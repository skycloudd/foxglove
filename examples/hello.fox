let a = 123;
let b = -456;
let c = a + b;

print a; // 123
print b; // -456
print c; // -333

let d = true;
let e = d;

{
    let d = false;
    e = d;
    print d; // false
}

print d; // true
print e; // false

let f = 0;

loop {
    print f; // 0, 1, 2, 3, 4

    f = f + 1;

    if f >= 5 {
        break;
    }
}

let g = 0;

while g < 5 {
    print g; // 0, 1, 2, 3, 4

    g = g + 1;
}
